{
  "name": "moonio-server",
  "version": "1.0.0",
  "description": "My Moleculer microservices project",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config moleculer.config.ts --envfile ./env/dev.env",
    "cli": "moleculer connect NATS",
    "start": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --config moleculer.config.ts",
    "lint": "eslint --ext .js,.ts .",
    "dc:up": "docker-compose up --build -d",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "moleculer-repl": "^0.5.3",
    "prettier": "^2.0.5",
    "tslint-config-prettier": "^1.18.0"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/faker": "^4.1.11",
    "@types/joi": "^14.3.4",
    "@types/mongodb": "^3.5.16",
    "@types/mongoose": "^5.7.19",
    "@types/node": "^10.12.12",
    "@types/ramda": "^0.27.5",
    "faker": "^4.1.0",
    "ioredis": "^4.2.0",
    "joi": "^14.3.1",
    "moleculer": "^0.13.0",
    "moleculer-db": "^0.8.7",
    "moleculer-db-adapter-mongo": "^0.4.8",
    "moleculer-db-adapter-mongoose": "^0.8.4",
    "moleculer-web": "^0.8.0",
    "mongodb": "^3.5.7",
    "mongoose": "^5.9.13",
    "nats": "^1.0.1",
    "ramda": "^0.27.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "engines": {
    "node": ">= 8.x.x"
  }
}
